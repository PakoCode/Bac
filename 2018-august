3.

bool prim(int n){
    if(n == 0 || n == 1) return 0;
    if(n == 2) return 1;
    if(n % 2 == 0) return 0;
    int d = 3;
    for(d = 3; d <= n / 2; d ++){
        if(n % d == 0) return 0;
    }
    return 1;
}
int interval(int n){
    int ok = 0, x = n + 1;
    if(prim (n) == 1) ok ++;
    while(ok <= 1){
        x ++;
        if(prim(x) == 1) ok ++;
    }
    return x - 1;
}


4.
#include <iostream>

using namespace std;

int main() {
    int x, y, z;
    cin >> x >> y;
    cout << y << " " << x << " ";
    while(x >= 0){
        cout << x << " ";
        z = -1*(y - 2 * x - 2);
        y = x;
        x = z;
    }
    
    return 0;
}







