5.
#include <iostream>

using namespace std;

int main() {
    int a[5], b[100][100], n;
    cin >> n;
    for(int i = 1; i <= n; i ++){
        cin >> a[i];
    }
    for(int j = 2; j <= n; j += 2){
        for(int i = 1; i <= n; i ++){
            b[i][j] = a[i];
        }
    }
     for(int j = 1; j <= n; j += 2){
        for(int i = 1; i <= n; i ++){
            b[i][j] = a[n - i + 1];
        }
    }
    
    for(int i = 1; i <= n; i ++){
        for(int j = 1; j <= n; j ++){
            cout << b[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}


